{
  "expo": {
    "name": "servcommerce",
    "slug": "servcommerce",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0046FF"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "config": {
        "usesNonExemptEncryption": false
      },
      "supportsTablet": false,
      "bundleIdentifier": "com.servcommerce.serv"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0046FF"
      },
      "package": "com.servcommerce.serv"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "02ad17dd-170e-49f0-867e-09150465cc72"
      }
    },
    "owner": "elijahwood",
    "plugins": [
      "@heap/react-native-heap"
    ]
  }
}